        "func": "function delay(delayInms) {\n    return new Promise(resolve => {\n        setTimeout(() => {\n            resolve(2);\n        }, delayInms);\n    });\n}\n\nif (flow.get(\"power\") === undefined) {\n    flow.set(\"power\", false)\n}\n\nmsg.payload = {};\nmsg.payload.roaster = [{ \"bean\": 200, \"air\": 200}, { \"bean\": 200, \"air\": 200}];\n\nfunction update_roaster() {\n    let roaster = msg.payload.roaster;\n    for(let i=0; i<roaster.length; i++) {\n        roaster[i].bean+=Math.floor((Math.random() - 0.5) * 3);\n        roaster[i].air+=Math.floor((Math.random() - 0.5) * 3);\n    }\n}\n\nasync function loop() {\n    while(flow.get(\"power\")) {\n        await delay(1000);\n        update_roaster();\n        node.send(msg);\n    }\n    msg.payload.roaster = [{ \"bean\": 25, \"air\": 25}, { \"bean\": 25, \"air\": 25}];\n    node.send(msg);\n}\n\nloop();",
